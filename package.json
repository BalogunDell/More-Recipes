{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "More-Recipes provides a platform for users to share the awesome and exciting recipe ideas they have invented or learnt. Suppose a user comes up with a food recipe, he/she can post it on More-Recipes and get feedback in form of reviews and votes from other users who explore that recipe. Users can also keep a list of their favorite recipes on the application.",
  "main": "app.js",
  "scripts": {
    "postinstall": "",
    "start": "nodemon --exec babel-node app.js",
    "test": "cross-env NODE_ENV=test mocha --timeout 40000 --compilers js:babel-core/register test ",
    "coverage": "nyc --reporter=html --reporter=text --reporter=lcov cross-env NODE_ENV=test mocha --reporter spec --require babel-register test --timeout 40000",
    "coveralls": "npm run coverage -- --report lcovonly && cat coverage/lcov.info | coveralls",
    "lint": "eslint ./",
    "testClient": "mocha --reporter progress ./testSetup.js \"client/**/test.js\"",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml",
    "prebuild": "npm-run-all clean-dist build:html",
    "heroku-postbuild": "babel-node tools/build.js && npm run build:html",
    "build-dev": "webpack --config ./webpack.config.dev.js --hot  --inline --progress --colors",
    "postbuild": "babel-node tools/distServer"
  },
  "engines": {
    "node": "8.1.x",
    "npm": "5.0.x"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "6.8.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "bootstrap": "^4.0.0-beta",
    "chai": "^4.1.1",
    "cheerio": "^1.0.0-rc.2",
    "cloudinary-react": "^1.0.4",
    "colors": "^1.1.2",
    "compression": "^1.7.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "eventsource-polyfill": "0.9.6",
    "expect": "^1.20.2",
    "express": "^4.15.4",
    "express-validator": "^4.1.1",
