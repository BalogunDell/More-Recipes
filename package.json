    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "mocha": "^3.5.0",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.19.2",
    "node-sass": "^4.5.3",
    "nodemailer": "^4.1.0",
